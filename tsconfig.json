{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": true,
        "declaration": true,
        "outDir": "./lib",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["es6", "dom"]
    },
    "exclude": [
        "node_modules",
        "lib",
        "test",
        "browser-test",
        "coverage",
        "example"
    ]
}
